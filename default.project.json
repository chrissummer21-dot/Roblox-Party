{
  "name": "Roblox Party",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "Game": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true,

        "Net": {
          "$className": "Folder",
          "$ignoreUnknownInstances": true,

          "RemoteEvents": {
            "$className": "Folder",
            "$ignoreUnknownInstances": true
          },
          "RemoteFunctions": {
            "$className": "Folder",
            "$ignoreUnknownInstances": true
          }
        },

        "Shared": {
          "$path": "src/ReplicatedStorage/Game/Shared",
          "$ignoreUnknownInstances": true
        },
        "UI": {
          "$path": "src/ReplicatedStorage/Game/UI",
          "$ignoreUnknownInstances": true
        },
        "Minigames": {
          "$path": "src/ReplicatedStorage/Game/Minigames",
          "$ignoreUnknownInstances": true
        },
        "Config": {
          "$path": "src/ReplicatedStorage/Game/Config",
          "$ignoreUnknownInstances": true
        }
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "GameServer": {
        "$path": "src/ServerScriptService/GameServer",
        "$ignoreUnknownInstances": true
      }
    },

    "ServerStorage": {
      "$className": "ServerStorage",
      "Maps": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true,

        "LobbyMap": {
          "$className": "Model",
          "$ignoreUnknownInstances": true
        },
        "MinigameMaps": {
          "$className": "Folder",
          "$ignoreUnknownInstances": true
        }
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$path": "src/StarterPlayerScripts",
        "$ignoreUnknownInstances": true
      }
    }
  }
}
